{
  "name": "babel-plugin-typecheck",
  "version": "3.0.0",
  "description": "Transforms flow type annotations into runtime type checks.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d ./lib ./src",
    "build-typed": "npm run build && babel --plugins typecheck -d ./lib ./src",
    "prepublish": "npm run build",
    "pretest": "npm run build-typed",
    "test": "mocha",
    "watch": "NODE_WATCH=1 mocha --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codemix/babel-plugin-typecheck"
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "types",
    "typing",
    "typecheck",
    "type check",
    "flow"
  ],
  "author": "Charles Pick <charles@codemix.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codemix/babel-plugin-typecheck/issues"
  },
  "homepage": "https://github.com/codemix/babel-plugin-typecheck",
  "devDependencies": {
    "babel": "^6.0.0",
    "babel-cli": "^6.0.2",
    "babel-core": "^6.0.2",
    "babel-plugin-syntax-flow": "^6.0.2",
    "babel-plugin-syntax-jsx": "^6.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.0.2",
    "babel-plugin-transform-react-jsx": "^6.0.2",
    "babel-preset-es2015": "^6.0.8",
    "babel-preset-react": "^6.0.2",
    "babel-runtime": "^6.0.8",
    "mocha": "~2.2.4",
    "should": "^6.0.1"
  },
  "dependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.0.2"
  }
}
